      
	{
  "manifest_version": 2,
  "name": "Ankit Background Pop Up",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "background.png",
    "default_popup": "CodingChallenge_10.1_MazeGenerator/index.html",
    "default_title": "A popup will come here."
  }
}